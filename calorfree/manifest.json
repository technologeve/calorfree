{
    "manifest_version": 3,
    "name": "Calorfree",
    "version": "1.0",
    "description": "Calorfree: block calorie information from websites.",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      },
    "action": {
        "default_popup": "options.html"
    },
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["scripts/content.js", "scripts/options.js"]
        }
    ],
    "permissions": [
        "storage"
    ]
}